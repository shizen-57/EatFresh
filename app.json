{
  "expo": {
    "name": "EatFresh",
    "slug": "eatfresh-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logooF.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fc7019"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logooF.jpg",
        "backgroundColor": "#fc7019"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAOVYRIgupAurZup5y1PRh8Ismb1A3lLao"
        }
      },
      "package": "com.yourcompany.eatfresh",
      "versionCode": 1,
      "softwareKeyboardLayoutMode": "pan",
      "proguard": {
        "rules": "-keep class org.osmdroid.** { *; }"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#fc7019"
      }
    },
    "owner": "shifu42",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "080beac9-f2fb-4811-80b4-e7cd030b8321"
      }
    }
  }
}
